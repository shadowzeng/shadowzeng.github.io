    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "ng build production",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/ng-home",
            "program": "${workspaceFolder}/ng-home/node_modules/.bin/ng",
            "args": ["build", "--configuration", "production"]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "webpack build production",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/ng-home",
            "program": "${workspaceFolder}/ng-home/node_modules/.bin/ng",
            "args": ["run", "ng-home:serve-webpack"]
        }
    ]
}
